FI"Å(function() {

  $(document).ready(function() {
    var add_fields, remove_fields;
    $(".close").click(function() {
      return $(".alert-message").fadeOut(1000, function() {});
    });
    remove_fields = function(link) {
      $(link).previous("input[type=hidden]").value = "1";
      return $(link).up(".ingredients").hide();
    };
    return add_fields = function(link, association, content) {
      var new_id, regexp;
      new_id = new Date().getTime();
      regexp = new RegExp("new_" + association, "g");
      return $(link).up().insert({
        before: content.replace(regexp, new_id)
      });
    };
  });

}).call(this);
