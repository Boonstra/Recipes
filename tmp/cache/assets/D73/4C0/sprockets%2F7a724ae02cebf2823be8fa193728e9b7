FI"²(function() {

  (function() {
    var loading, nearBottomOfPage, page;
    nearBottomOfPage = function() {
      return $(window).scrollTop() > $(document).height() - $(window).height() - 200;
    };
    page = 1;
    loading = false;
    $(window).scroll(function() {
      if (loading) return;
      if (nearBottomOfPage()) {
        loading = true;
        page++;
        return $.ajax({
          url: "/timeline?page=" + page,
          type: "get",
          dataType: "script",
          success: function() {
            $(window).sausage("draw");
            return loading = false;
          }
        });
      }
    });
    return $(window).sausage();
  })();

}).call(this);
